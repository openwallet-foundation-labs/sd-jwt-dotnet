<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <!-- Remove all inherited sources -->
    <clear />
    <!-- Add nuget.org as the only source -->
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
  </packageSources>
  
  <packageSourceMapping>
    <!-- Remove all inherited source mappings -->
    <clear />
    <!-- Map all packages to nuget.org -->
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
  </packageSourceMapping>
  
  <disabledPackageSources>
    <!-- Ensure no package sources are disabled -->
    <clear />
  </disabledPackageSources>
</configuration>

